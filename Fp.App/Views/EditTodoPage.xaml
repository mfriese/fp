<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="Fp.App.Views.EditTodoPage"
    Title="EditTodoPage">
    <VerticalStackLayout>
        <Label
            Text="Id"
            Margin="4"
            FontSize="Medium"
            HorizontalOptions="Start" />
        <Label
            Text="{Binding Todo.Id}"
            Margin="4"
            FontSize="Medium"
            HorizontalOptions="Start" />
    </VerticalStackLayout>
</ContentPage>